<!DOCTYPE html>
<html lang="es">
<head>
  <meta charset="UTF-8">
  <title>Inmuebles</title>
  <style>
    body { font-family: sans-serif; background: #f0f0f0; }
    table { margin: 20px auto; border-collapse: collapse; background: white; }
    th, td { padding: 10px 20px; border: 1px solid #ccc; }
    th { background: #4CAF50; color: white; }
  </style>
</head>
<body>
  <h1 style="text-align: center;">Listado de Inmuebles</h1>
  <table>
    <tr>
      <th>ID</th>
      <th>Direcci√≥n</th>
      <th>Precio</th>
      <th>Tipo</th>
    </tr>
    <% inmuebles.forEach(i => { %>
      <tr>
        <td><%= i.id %></td>
        <td><%= i.direccion %></td>
        <td>$<%= i.precio.toLocaleString() %></td>
        <td><%= i.tipo %></td>
      </tr>
    <% }) %>
  </table>
</body>
</html>
